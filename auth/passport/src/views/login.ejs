<section class="prompt">
  <h1>Sign In</h1>
  <form action="/login" method="post">
    <div>
      <label for="email">Email</label>
      <input
        type="email"
        name="email"
        id="email"
        placeholder="Email"
        required
      />
    </div>
    <div>
      <label for="password">Password</label>
      <input
        type="password"
        name="password"
        id="password"
        placeholder="password"
        required
      />
    </div>
    <button type="submit">Sign In</button>
    <p>아직 계정이 없다면<a href="/signup">Sign UP</a></p>
  </form>
</section>
